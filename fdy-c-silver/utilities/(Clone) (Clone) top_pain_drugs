"-- top 10 drugs ordered within +/- 1 day of high pain score\r\nuse cdh_pointclickcare_exploratory;\r\n\r\n--create or replace table cdh_pointclickcare_exploratory.qxv3_toppaindrugs as\r\nselect State, DrugName, count(*) as n_orders from qxv3_paindrugs\r\nwhere DrugName is not null and State in ({{ State_parameter }})\r\ngroup by grouping sets((DrugName, State), (DrugName))\r\nhaving State is not null\r\norder by n_orders desc\r\nlimit 10"