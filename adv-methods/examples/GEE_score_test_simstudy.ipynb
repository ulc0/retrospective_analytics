     "cell_type": "code",
     "collapsed": false,
     "input": [
      "plt.clf()\n",
      "plt.plot(wald_tests, score_tests, 'o', color='orange', alpha=0.6)\n",
      "plt.grid(True)\n",
      "plt.xlim(0, 1)\n",
      "plt.ylim(0, 1)\n",
      "plt.xlabel(\"Wald test p-value\", size=14)\n",
      "plt.ylabel(\"Score test p-value\", size=14)"
