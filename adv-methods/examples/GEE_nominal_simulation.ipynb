   "outputs": [],
   "source": [
    "gor = sm.cov_struct.GlobalOddsRatio(\"nominal\")\n",
    "model = sm.NominalGEE(endog, exog, groups, cov_struct=gor)\n",
    "result = model.fit(start_params=pa, maxiter=100)\n",
    "print result.summary()\n",
    "print gor.summary()"
